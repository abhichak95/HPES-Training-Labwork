<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd">
<hibernate-reverse-engineering>
  <schema-selection match-catalog="backslash"/>
  <table-filter match-name="all_users"/>
  <table-filter match-name="user_other_details"/>
  <table-filter match-name="user_work_details"/>
  <table-filter match-name="user_educational_details"/>
  <table-filter match-name="all_communities"/>
  <table-filter match-name="user_personal_details"/>
</hibernate-reverse-engineering>
