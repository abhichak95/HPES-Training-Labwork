<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>GetVote</servlet-name>
        <servlet-class>com.hp.j2sh.GetVote</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterVoter</servlet-name>
        <servlet-class>com.hp.j2sh.RegisterVoter</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.hp.j2sh.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetVote</servlet-name>
        <url-pattern>/GetVote</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterVoter</servlet-name>
        <url-pattern>/RegisterVoter</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>homePage.jsp</welcome-file>
    </welcome-file-list>
</web-app>
