<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.hp.stp.j2sh.entity.User" table="user">
        <id name="userIDInteger" column="userID" type="integer"></id>
        <property name="usernameString" column="username" type="string"></property>
        <property name="passwordString" column="password" type="string"></property>
        <property name="statusShort" column="status" type="short"></property>
        <property name="userTypeString" column="userType" type="string"></property>
    </class>
</hibernate-mapping>
